{
  "$schema": "../schema/config-schema.json",
  "replacement": "[REDACTED]",
  "globalMode": "replace",
  "preserveLength": false,
  "preserveFormat": true,
  "categories": {
    "EMAIL": false,
    "PHONE": false,
    "SSN": true,
    "CREDIT_CARD": true,
    "IP_ADDRESS": false,
    "API_KEY": true,
    "AWS_KEY": true,
    "PRIVATE_KEY": true,
    "ADDRESS": false,
    "GOVERNMENT_ID": false,
    "FINANCIAL": true,
    "MEDICAL": false,
    "DIGITAL_IDENTITY": false,
    "GEOGRAPHIC": false,
    "EMPLOYEE_ID": false,
    "VEHICLE": false,
    "DEV_SECRET": true,
    "DEV_IDENTIFIER": false
  },
  "security": {
    "preventRegexDos": true,
    "maxPatternLength": 500,
    "maxCustomPatterns": 20,
    "auditLog": false
  },
  "performance": {
    "maxInputSize": 20971520,
    "timeout": 60000,
    "cachePatterns": true
  }
}
